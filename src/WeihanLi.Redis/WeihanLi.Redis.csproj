<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFrameworks>netstandard2.0;net45</TargetFrameworks>
        <Product>WeihanLi.Redis</Product>
        <Title>WeihanLi.Redis</Title>
        <PackageId>WeihanLi.Redis</PackageId>
        <Authors>WeihanLi</Authors>
        <Version>1.0.0-beta-3</Version>
        <RepositoryType>git</RepositoryType>
        <PackageTags>redis StackExchange</PackageTags>
        <PackageIconUrl>https://avatars3.githubusercontent.com/u/7604648</PackageIconUrl>
        <Description>RedisExtensions for StackExchange.Redis</Description>
        <PackageReleaseNotes>initial release</PackageReleaseNotes>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <RepositoryUrl>https://github.com/WeihanLi/WeihanLi.Redis</RepositoryUrl>
        <Copyright>MIT</Copyright>
        <PackageLicenseUrl>https://github.com/WeihanLi/WeihanLi.Redis/blob/master/LICENSE</PackageLicenseUrl>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="StackExchange.Redis" Version="1.2.6" />
        <PackageReference Include="WeihanLi.Common" Version="1.0.8" />
    </ItemGroup>

    <ItemGroup>
        <Compile Update="Resource.Designer.cs">
            <DesignTime>True</DesignTime>
            <AutoGen>True</AutoGen>
            <DependentUpon>Resource.resx</DependentUpon>
        </Compile>
    </ItemGroup>

    <ItemGroup>
        <EmbeddedResource Update="Resource.resx">
            <Generator>ResXFileCodeGenerator</Generator>
            <LastGenOutput>Resource.Designer.cs</LastGenOutput>
        </EmbeddedResource>
    </ItemGroup>
</Project>