<Project>  
  <Import Project="sign.props" />
  <Import Project="version.props" />
  <PropertyGroup>        
    <TargetFrameworks>netstandard2.0;</TargetFrameworks>
    <Product>WeihanLi.Redis</Product>
    <Title>WeihanLi.Redis</Title>
    <PackageId>WeihanLi.Redis</PackageId>
    <Authors>WeihanLi</Authors>
    <RepositoryType>git</RepositoryType>
    <PackageTags>redis StackExchange</PackageTags>
    <PackageIconUrl>https://avatars3.githubusercontent.com/u/7604648</PackageIconUrl>
    <Description>RedisExtensions for StackExchange.Redis</Description>
    <Summary>RedisExtensions for StackExchange.Redis</Summary>
    <PackageReleaseNotes>
      Add MaxCacheExpiry, you can set AllowNoExpiry false to avoid never expired keys
    </PackageReleaseNotes>
    <RepositoryUrl>https://github.com/WeihanLi/WeihanLi.Redis</RepositoryUrl>
    <Copyright>MIT</Copyright>
    <PackageProjectUrl>https://github.com/WeihanLi/WeihanLi.Redis</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <GeneratePackageOnBuild Condition="'$(Configuration)'=='Release'">True</GeneratePackageOnBuild>
  </PropertyGroup>
</Project>